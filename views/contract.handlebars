	<script>

		$('.update').click(function () {
		
		        var ele = $(this);
		        $('#pop').addClass('open');
		        $('#pop').addClass('slide');
		        setTimeout(function () {
		            $('#pop').fadeIn();
		            setTimeout(function () {
		                $('#pop').removeClass('open');
		                ele.removeClass('slide');
		            }, 100);
		        }, 100);
		
		    $('.close').click(function () {	        
		        $('#pop').fadeOut();
		    });
		});
	</script>
